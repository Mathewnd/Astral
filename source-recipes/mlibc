name=mlibc
version=f3e45cac82dcc1a90d8d1629e1b3e75d4fffe9df
source_method=tarball
tarball_url="https://github.com/managarm/mlibc/archive/${version}.tar.gz"
tarball_sha512="f841259fbc7baad163a828e58c9449714829207ddef6b8b854b5893c94e0e2afa3689a3756675aed864af81f1074329d8e818c3b09d68836388d70e999223787"

regenerate() {
	mkdir -p ${source_dir}/sysdeps/astral/include/
	ln -sv ../../../abis/linux/ ${source_dir}/sysdeps/astral/include/abi-bits
}
