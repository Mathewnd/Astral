name=mlibc-headers
from_source=mlibc
revision=1
deps="linux-headers"
imagedeps="ninja meson base-devel"

configure() {
	meson --cross-file "${base_dir}/util/crossfile" --prefix="${prefix}" -Dlinux_kernel_headers="${sysroot}/${prefix}" -Dheaders_only=true "${source_dir}"
}

build() {
	
	ninja

}

install() {
	DESTDIR=${dest_dir} ninja install
}
