name=doomgeneric
revision=1
from_source=doomgeneric
imagedeps="base-devel"
hostdeps="gcc-cross binutils-cross"
deps="mlibc"


configure() {
	
	true

}

build() {
	
	make -C ${source_dir}/doomgeneric

}

install() {
	mkdir -p ${dest_dir}/usr/bin
	cp -f ${source_dir}/doomgeneric/doomgeneric ${dest_dir}/usr/bin/doomgeneric

}
